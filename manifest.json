{
	"name": "Save My Tabs",
	"version": "1.0",
	"description": "Build an Extension!",
	"permissions": ["storage", "tabs"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": "clock.png",
		"default_popup": "./popup/popup.html"
	},
	"manifest_version": 2
}
